// DFA
digraph {
	startingStateH [label=startingStateH style=invis]
	S4 [label=S4 shape=doublecircle]
	S5 [label=S5 shape=doublecircle]
	S0 [label=S0]
	S1 [label=S1]
	S2 [label=S2]
	S4 -> S0 [label=0]
	S5 -> S4 [label=0]
	S5 -> S5 [label=1]
	S0 -> S1 [label=0]
	S0 -> S2 [label=1]
	S1 -> S0 [label=0]
	S2 -> S4 [label=0]
	S2 -> S5 [label=1]
	startingStateH -> S0
}
