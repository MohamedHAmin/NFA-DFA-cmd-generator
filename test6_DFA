// DFA
digraph {
	startingStateH [label=startingStateH style=invis]
	S0 [label=S0]
	S1 [label=S1]
	S2 [label=S2]
	S3 [label=S3 shape=doublecircle]
	S4 [label=S4 shape=doublecircle]
	S5 [label=S5]
	S0 -> S1 [label=1]
	S0 -> S2 [label=0]
	S1 -> S3 [label=1]
	S1 -> S4 [label=0]
	S2 -> S5 [label=0]
	S3 -> S3 [label=1]
	S3 -> S4 [label=0]
	S4 -> S5 [label=0]
	S5 -> S1 [label=1]
	S5 -> S2 [label=0]
	startingStateH -> S0
}
