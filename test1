digraph {
	startingStateH [label=startingStateH style=invis]
	S0 [label=S0]
	S1 [label=S1]
	S2 [label=S2]
	S3 [label=S3]
	S4 [label=S4]
	S5 [label=S5 shape=doublecircle]
	S0 -> S4 [label="ε"]
	S1 -> S2 [label=c]
	S2 -> S5 [label="ε"]
	S3 -> S5 [label="ε"]
	S4 -> S1 [label="ε"]
	S4 -> S3 [label="ε"]
	startingStateH -> S0
}
