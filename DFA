// DFA
digraph {
	startingStateH [label=startingStateH style=invis]
	S0 [label=S0]
	S1 [label=S1]
	S2 [label=S2 shape=doublecircle]
	S3 [label=S3 shape=doublecircle]
	S0 -> S1 [label="a-be-z"]
	S0 -> S2 [label=c]
	S0 -> S3 [label=d]
	S1 -> S1 [label="a-be-z"]
	S1 -> S2 [label=c]
	S1 -> S3 [label=d]
	S2 -> S1 [label="a-be-z"]
	S2 -> S2 [label=c]
	S2 -> S3 [label=d]
	S3 -> S1 [label="a-be-z"]
	S3 -> S2 [label=c]
	S3 -> S3 [label=d]
	startingStateH -> S0
}
