// DFA
digraph {
	startingStateH [label=startingStateH style=invis]
	S0 [label=S0]
	S1 [label=S1]
	S2 [label=S2]
	S3 [label=S3 shape=doublecircle]
	S0 -> S1 [label=a]
	S0 -> S2 [label=b]
	S1 -> S1 [label=a]
	S1 -> S2 [label=b]
	S2 -> S1 [label=a]
	S2 -> S2 [label=b]
	S2 -> S3 [label=c]
	S3 -> S3 [label=c]
	startingStateH -> S0
}
